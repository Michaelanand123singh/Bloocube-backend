# Build/deploy configuration for backend
PROJECT_ID=
REGION=us-central1
SERVICE=bloocube-backend
IMAGE=gcr.io/$PROJECT_ID/$SERVICE:$(date +%Y%m%d-%H%M%S)
PORT=5000
# Comma-separated CORS origins for backend
CORS_ORIGIN=http://localhost:3000,https://bloocube.com,https://admin.bloocube.com,https://api-backend.bloocube.com,https://api-ai-services.bloocube.com
# FRONTEND URL used for redirects
FRONTEND_URL=https://bloocube.com
# Required secrets to set on Cloud Run (set via Console or gcloud)
# JWT_SECRET=
# JWT_REFRESH_SECRET=
# MONGODB_URI=
# REDIS_URL=
